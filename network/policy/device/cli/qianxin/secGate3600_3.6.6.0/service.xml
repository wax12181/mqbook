    <!-- Service Group -->

    <CREATE-SERVICE-GROUP>
        config terminal
            object service-group ${name}
                service ${serviceName}                  <!-- NAME{1-63}  Service name -->
                service-group ${serviceGroupName}       <!-- NAME{1-63}  Service group name -->
